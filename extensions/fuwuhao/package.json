{
  "name": "@tencent/openclaw-fuwuhao",
  "version": "1.0.0",
  "type": "module",
  "description": "OpenClaw 微信服务号智能机器人插件 - 通过加密 Webhook 接收消息",
  "author": "Tencent",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AntDesign/openclaw-fuwuhao"
  },
  "keywords": [
    "openclaw",
    "wechat",
    "fuwuhao",
    "服务号",
    "微信公众号",
    "chatbot",
    "ai"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "openclaw.plugin.json"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ],
    "channel": {
      "id": "fuwuhao",
      "label": "服务号",
      "selectionLabel": "微信服务号 (plugin)",
      "detailLabel": "微信服务号 Bot",
      "docsPath": "/channels/fuwuhao",
      "docsLabel": "fuwuhao",
      "blurb": "微信服务号智能机器人（API 模式）通过加密 Webhook 接收消息。",
      "aliases": [
        "fuwuhao",
        "服务号",
        "微信公众号",
        "公众号"
      ],
      "order": 85
    },
    "install": {
      "npmSpec": "@tencent/openclaw-fuwuhao",
      "localPath": "extensions/fuwuhao",
      "defaultChoice": "npm"
    }
  },
  "dependencies": {
    "fast-xml-parser": "5.3.4",
    "undici": "^7.20.0",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.26"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "typescript": "^5.9.3"
  }
}